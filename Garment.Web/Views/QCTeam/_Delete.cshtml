@model QCTeam

@using (Ajax.BeginForm("_DeleteConfirm", new AjaxOptions
{
    HttpMethod = "Post",
    OnSuccess = "onSuccessCreateEditQCTeam"
}))
{
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Xóa báo cáo QC</h4>
    </div>
    <div class="modal-body">
        <h5>Bạn có muốn xóa <strong>@Model.QC.Type</strong> khỏi <strong>@Model.Team.Name</strong></h5>
    </div>
    <div class="modal-footer">
        @Html.HiddenFor(model => model.TeamId)
        @Html.HiddenFor(model => model.QCId)
        <button type="button" class="btn btn-default" data-dismiss="modal">Hủy</button>
        <button type="submit" class="btn btn-primary">Xóa</button>
    </div>
}

